citHeader("To cite tbrf in publications use:")

citEntry(
  entry    = "manual",
  title    = "{tbrf}: Time Based Rolling Functions",
  author   = person("Michael", Schramm),
  year     = "2020",
  url = "https://CRAN.R-project.org/package=tbrf",
  doi = "10.5281/zenodo.3727319",
  textVersion = paste(
  "Schramm, Michael (2020). ", 
  "tbrf: Time based rolling functions. " 
  "doi:10.5281/zenodo.3727319"
  )
)
